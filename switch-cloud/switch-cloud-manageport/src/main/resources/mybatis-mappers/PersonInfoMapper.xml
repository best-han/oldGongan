<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.windaka.suizhi.manageport.dao.PersonInfoDao">
    <!-- 插入人员信息 -->
    <insert id="insertPerson" parameterType="Map">
        INSERT INTO personinfo(id,name,paper_type,paper_num,sex,birthday,nation,address,phone,typename,addresscode,marriage,education,political)
        values (#{id},#{name},#{paperType},#{paperNum},#{sex},#{birthday},#{nation},#{address},#{phone},#{typename}
        ,#{addresscode},#{marriage},#{education},#{political})
    </insert>
</mapper>
